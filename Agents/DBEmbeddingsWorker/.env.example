# Copy to .env and fill. Same DB as QueryAnalyst / Platform.
SUPABASE_DB_PASSWORD=
SUPABASE_DB_HOST=aws-1-ap-southeast-1.pooler.supabase.com
SUPABASE_DB_PORT=6543
SUPABASE_DB_USER=postgres.xxxxx
SUPABASE_DB_NAME=postgres

# Grievance table (must have embedding_status column from migration)
GRIEVANCE_TABLE=usergrievance

# Model: same as QueryAnalyst for consistent vectors (384-dim)
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Worker tuning
EMBEDDING_POLL_INTERVAL_SEC=2.0
EMBEDDING_BATCH_SIZE=10
