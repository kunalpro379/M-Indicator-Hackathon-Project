# Copy this file to .env and fill in your values

# LLM APIs
GROQ_API_KEY=your_groq_api_key
GEMINI_API_KEY=your_gemini_api_key
TAVILY_API_KEY=your_tavily_api_key

# Supabase
SUPABASE_DB_PASSWORD=your_supabase_password

# Neon DB connection strings (postgresql://user:pass@host/db?sslmode=require)
NEON_DB_1_URL=postgresql://user:pass@host/database1?sslmode=require
NEON_DB_2_URL=postgresql://user:pass@host/database2?sslmode=require
NEON_DB_3_URL=postgresql://user:pass@host/database3?sslmode=require
NEON_DB_4_URL=postgresql://user:pass@host/database4?sslmode=require
NEON_DB_5_URL=postgresql://user:pass@host/database5?sslmode=require
NEON_DB_7_URL=postgresql://user:pass@host/database7?sslmode=require

# Azure Storage (for queue and blob)
AZURE_STORAGE_CONNECTION_STRING=your_azure_connection_string
AZURE_QUEUE_NAME=queryanalyst
AZURE_WEBCRAWLER_QUEUE_NAME=webcrawler
AZURE_STORAGE_CONTAINER_NAME=test
