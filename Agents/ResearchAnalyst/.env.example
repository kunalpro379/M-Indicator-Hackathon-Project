# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database

# Supabase Configuration (if using Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# API Keys (will be fetched from settings table)
# GROQ_API_KEY will be fetched from settings table

# Search Configuration
TAVILY_API_KEY=your-tavily-api-key

# Agent Configuration
MAX_SEARCH_RESULTS=10  # Number of search results to fetch from Tavily (will be validated before pushing to queue)
MAX_ITERATIONS=5
BATCH_SIZE=5

# Azure Queue Configuration
AZURE_STORAGE_CONNECTION_STRING=your-azure-connection-string
WEBCRAWLER_QUEUE_NAME=webcrawler
